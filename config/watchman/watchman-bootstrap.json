[
  "trigger",
  "/app",
  {
    "name": "watchman-bootstrap",
    "expression": [
      "anyof",
      [
        "dirname",
        "config/watchman"
      ],
      [
        "name",
        "develop.sh"
      ]
    ],
    "command": [
      "./bin/develop.sh",
      "watchman"
    ],
    "append_files": false
  }
]
